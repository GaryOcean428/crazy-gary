{
  "name": "@crazy-gary/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5675",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production tsc && vite build",
    "build:optimized": "node scripts/build-optimizer.js --optimize --analyze --budget",
    "build:analyze": "ANALYZE_BUNDLE=true npm run build",
    "build:performance": "npm run clean && node scripts/build-optimizer.js --optimize --analyze --budget --preload --monitor",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --port 5676",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "node scripts/test-runner.js ui",
    "test:accessibility": "node scripts/test-runner.js accessibility",
    "test:accessibility:watch": "vitest --testPathPattern=accessibility --watch",
    "test:accessibility:coverage": "vitest run --coverage --testPathPattern=accessibility",
    "test:accessibility:report": "npm run test:accessibility && npm run accessibility",
    "test:accessibility:ci": "chmod +x scripts/accessibility-ci.sh && ./scripts/accessibility-ci.sh",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:debug": "playwright test --debug",
    "test:visual:headed": "playwright test --headed",
    "test:visual:ci": "playwright test --reporter=html",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .next coverage node_modules/.vite",
    "clean:cache": "rm -rf node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "analyze:detailed": "npm run build:production && node scripts/performance-monitor.js --analyze",
    "check-bundle": "npm run build && ls -la dist/assets/",
    "check-performance": "node scripts/performance-monitor.js --audit",
    "performance:monitor": "node scripts/performance-monitor.js --analyze",
    "performance:budget": "npm run build:production && node scripts/performance-monitor.js --audit",
    "accessibility": "npm run build && npx axe-core dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "optimize:deps": "npm run clean && npm run install --production=false",
    "optimize:assets": "node scripts/build-optimizer.js --optimize",
    "preload:generate": "node scripts/resource-preloader.js",
    "preload:test": "npm run build:production && node scripts/resource-preloader.js",
    "watch:performance": "watch 'npm run performance:monitor' dist/",
    "serve:dist": "npm run build:production && npx serve dist -p 3000",
    "serve:analyze": "npm run build:analyze && npx serve dist -p 3000",
    "test:optimization": "node scripts/test-optimization.js",
    "test:all": "npm run test:optimization && npm run test",
    "test:unit": "node scripts/test-runner.js unit",
    "test:integration": "node scripts/test-runner.js integration",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:serve": "npm run build-storybook && serve storybook-static -p 6006",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "test:storybook:ci": "start-server-and-test 'storybook dev -p 6006' http://localhost:6006 'test-storybook'",
    "chromatic": "npx chromatic --project-token=YOUR_PROJECT_TOKEN",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:visual": "playwright test visual-regression.spec.ts",
    "test:e2e:performance": "playwright test performance.spec.ts",
    "test:e2e:auth": "playwright test auth.spec.ts",
    "test:e2e:api": "playwright test api-integration.spec.ts",
    "test:e2e:heavy": "playwright test heavy-mode.spec.ts",
    "test:e2e:tasks": "playwright test task-manager.spec.ts",
    "test:e2e:settings": "playwright test settings-profile.spec.ts",
    "test:e2e:dashboard": "playwright test dashboard.spec.ts",
    "test:e2e:install": "playwright install",
    "test:e2e:install-deps": "playwright install-deps",
    "test:e2e:install-browsers": "playwright install chromium firefox webkit",
    "test:e2e:codegen": "playwright codegen http://localhost:5675",
    "test:e2e:show-report": "playwright show-report",
    "test:e2e:enhanced": "node scripts/test-runner-enhanced.js",
    "test:e2e:enhanced:e2e": "node scripts/test-runner-enhanced.js --type e2e",
    "test:e2e:enhanced:visual": "node scripts/test-runner-enhanced.js --type visual",
    "test:e2e:enhanced:performance": "node scripts/test-runner-enhanced.js --type performance",
    "test:e2e:enhanced:accessibility": "node scripts/test-runner-enhanced.js --type accessibility",
    "test:e2e:enhanced:api": "node scripts/test-runner-enhanced.js --type api",
    "test:e2e:full": "npm run test:e2e:install && npm run test:e2e && npm run test:visual:update",
    "test:e2e:ci": "npm run test:e2e:install && CI=true playwright test --reporter=html,junit",
    "test:e2e:coverage": "npm run test:e2e && npm run coverage:report",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:slow": "playwright test --timeout=120000",
    "test:e2e:trace": "playwright test --trace=on",
    "test:e2e:video": "playwright test --video=on",
    "test:e2e:screenshot": "playwright test --screenshot=on",
    "codegen": "playwright codegen",
    "show-report": "playwright show-report",
    "test:hooks": "node scripts/test-runner.js hooks",
    "test:utils": "node scripts/test-runner.js utils",
    "test:cache": "node scripts/test-runner.js cache",
    "test:api": "node scripts/test-runner.js api",
    "test:contexts": "node scripts/test-runner.js contexts",
    "test:performance": "node scripts/test-runner.js performance",
    "test:runner": "node scripts/test-runner.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/vite": "^4.1.7",
    "axios": "^1.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.0.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.15.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.510.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.1",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^4.1.0",
    "react-hook-form": "^7.56.3",
    "react-resizable-panels": "^3.0.2",
    "react-router-dom": "^7.6.1",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.4",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.47.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.8.0",
    "cssnano": "^6.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.4.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.1",
    "terser": "^5.44.0",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.5.0",
    "vite": "^6.3.6",
    "vite-bundle-visualizer": "^1.0.0",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^2.0.0",
    "watch": "^1.0.2"
  },
  "packageManager": "yarn@4.0.2"
}

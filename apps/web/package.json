{
  "name": "@crazy-gary/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5675",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --port 5676",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .next coverage",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "check-bundle": "npm run build && ls -la dist/assets/",
    "accessibility": "npm run build && npx axe-core dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\""
  },
  "dependencies": {

	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Set up authentication variables",
					"if (!pm.environment.get('accessToken')) {",
					"    pm.environment.set('accessToken', pm.variables.get('accessToken') || '');",
					"}",
					"",
					"// Log request for debugging",
					"console.log('Request:', pm.request.method, pm.request.url.toString());",
					"",
					"// Add request ID header",
					"const requestId = 'req_' + Date.now() + '_' + Math.random().toString(36).substr(2, 9);",
					"pm.request.headers.add({",
					"    key: 'X-Request-ID',",
					"    value: requestId",
					"});"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Test response status codes",
					"pm.test('Status code is 2xx', function () {",
					"    pm.response.to.be.success;",
					"});",
					"",
					"// Test response time",
					"pm.test('Response time is less than 2000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(2000);",
					"});",
					"",
					"// Test content type",
					"pm.test('Content-Type is application/json', function () {",
					"    pm.response.to.be.json;",
					"    pm.response.to.have.header('Content-Type', 'application/json');",
					"});",
					"",
					"// Test rate limiting headers",
					"if (pm.response.to.have.header('X-RateLimit-Limit')) {",
					"    pm.test('Rate limit headers present', function () {",
					"        pm.response.to.have.header('X-RateLimit-Limit');",
					"        pm.response.to.have.header('X-RateLimit-Remaining');",
					"        pm.response.to.have.header('X-RateLimit-Reset');",
					"    });",
					"}",
					"",
					"// Log response for debugging",
					"console.log('Response status:', pm.response.status);",
					"console.log('Response time:', pm.response.responseTime + 'ms');",
					"",
					"// Extract and store variables from response",
					"if (pm.response.code === 200 || pm.response.code === 201) {",
					"    const jsonData = pm.response.json();",
					"    ",
					"    // Store user data from auth responses",
					"    if (jsonData.token && !pm.environment.get('accessToken')) {",
					"        pm.environment.set('accessToken', jsonData.token);",
					"        pm.environment.set('refreshToken', jsonData.refresh_token);",
					"        pm.environment.set('userId', jsonData.user.id);",
					"        pm.environment.set('userEmail', jsonData.user.email);",
					"    }",
					"    ",
					"    // Store task data",
					"    if (jsonData.task_id) {",
					"        pm.environment.set('taskId', jsonData.task_id);",
					"    }",
					"}"
				]
			}
		}
	],
